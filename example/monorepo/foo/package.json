{
  "name": "example-monorepo-foo",
  "version": "0.0.0",
  "scripts": {
    "dev": "DEBUG=vite-plugin-optimize-persist vite --force",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "example-monorepo-bar": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vite-plugin-optimize-persist": "workspace:*"
  },
  "vite": {
    "define": {
      "__NAME__": "\"Package Config\""
    },
    "optimizeDeps": {
      "include": [
        "example-monorepo-bar > lodash/map",
        "example-monorepo-bar > lodash/set",
        "example-monorepo-bar > lodash/upperCase"
      ],
      "exclude": [
        "example-monorepo-bar"
      ]
    }
  }
}
